<odoo>
    <record id="product_pricelist_cache_wizard_view_form" model="ir.ui.view">
      <field name="name">product.pricelist.cache.wizard.form.inherit</field>
      <field name="model">product.pricelist.cache.wizard</field>
      <field
            name="inherit_id"
            ref="pricelist_cache.product_pricelist_cache_wizard_view_form"
        />
      <field name="arch" type="xml">
          <field name="product_id" position="after">
              <field name="at_date" />
          </field>

          <xpath expr="//tree/field[@name='price']" position="after">
            <field name="date_start" />
            <field name="date_end" />
          </xpath>
      </field>
    </record>
</odoo>
